{
    "manifest_version": 3,
    "name": "SCOUT",
    "version": "1.0",
    "icons": {
        "16":"icons/16.png",
        "32":"icons/32.png",
        "48":"icons/48.png",
        "128":"icons/128.png"},
    "permissions": [
      "sidePanel"
    ],
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "content_security_policy": { 
        "extension_pages": "script-src 'self'; object-src 'self'"
      }
  }