{
    "name": "chat bot",
    "description": "ai bot",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {"16":"icons/16.png",
    "32":"icons/32.png",
    "48":"icons/48.png",
    "128":"icons/128.png"},
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": [],
    "background":{
        "service_worker": "back.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/all.min.js"],
            "css": ["css/popup.css"]
        }
    ]
}