{
    "name": "chat bot",
    "description": "ai bot",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {"16":"icons/16.png",
    "32":"icons/32.png",
    "48":"icons/48.png",
    "128":"icons/128.png"},
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": [],
    "background":{

    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["back.js"],
            "css": ["popup.css"]
        }
    ],
    "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+M",
          "mac": "Command+U"
        }
    }
}